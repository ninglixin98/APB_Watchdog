#!/bin/sh -e
# This file is automatically generated by VCS.  Any changes you make
# to it will be overwritten the next time VCS is run.
echo "Unified Use model is not supported"
exit -1
vcs '-full64' '-ntb_opts' 'uvm-1.2' '-debug_acc+all' '-l' 'elab.log' '-sim_res=1ps' '-top' 'rkv_watchdog_tb' '-o' 'rkv_watchdog_tb.simv' "-Xrebuild" "-makedepends=Makefile.Msimdepends"
gmake -f Makefile.Msimdepends
vcs '-full64' '-ntb_opts' 'uvm-1.2' '-debug_acc+all' '-l' 'elab.log' '-sim_res=1ps' '-top' 'rkv_watchdog_tb' '-o' 'rkv_watchdog_tb.simv'  -static_dbgen_only -daidir=$1 2>&1
